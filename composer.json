{
  "name": "sotagency/blade",
  "description": "A library for using Laravel Blade templates in WordPress/WordPlate.",
	"homepage": "https://github.com/sotagency/blade",
  "keywords": ["wordplate", "blade", "laravel", "wordpress", "plugin"],
  "license": "MIT",
  "authors": [
    {
      "name": "Oskar Joelson",
      "email": "oskar.joelson@wearemore.se",
      "homepage": "https://wearemore.se/"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "jenssegers/blade": "^1.1",
    "wordplate/framework": "^6.2"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra":{
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ]
  }
}
