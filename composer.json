{
    "name": "sotagency/blade",
    "description": "A Laravel Blade support plugin for WordPlate",
    "keywords": ["wordplate", "blade", "laravel", "wordpress", "plugin"],
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Joelson",
            "email": "oskar.joelson@sot.se",
            "homepage": "https://sot.se/"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "jenssegers/blade": "^1.1",
        "wordplate/framework": "^6.2"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra":{
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
