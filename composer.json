{
  "name": "fiskhandlarn/blade",
  "description": "A library for using Laravel Blade templates in WordPress/WordPlate.",
	"homepage": "https://github.com/fiskhandlarn/blade",
  "keywords": ["wordplate", "blade", "laravel", "wordpress", "template", "view", "render"],
  "license": "MIT",
  "authors": [
    {
      "name": "Oskar Joelson",
      "email": "oskar@joelson.org",
      "homepage": "https://wearemore.se/"
    },
    {
      "name": "Daniel Gerdgren",
      "email": "daniel.gerdgren@hoy.se",
      "homepage": "https://hoy.se/"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/view": "^5.7",
    "wordplate/framework": "^6.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra":{
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Fiskhandlarn\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/helpers.php"
    ],
    "psr-4": {
      "Fiskhandlarn\\Tests\\": "tests/"
    }
  }
}
